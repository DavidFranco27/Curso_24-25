#Leo cinco numeros y me dice cual es mayor
def mayor():
    mayor=0
    for cont in range(1,6):
        n=int(input("Dime un numero entero mayor que: "))
        if (n>mayor):
            mayor=numero
            print(str(n) +"es mayor")
        else:
            print(str(n) +" no es mayor que ")

mayor()
