#Este programa sirve para hacer codigo binario de una palabra con mayusculas y sea curva
def curvi():
    palabra=raw_input("Dime una palabra con mayuscyulas curvas y hare un codigo binario ")
    for letra in palabra:
        if(letra=='B' or letra=='C' or letra=='D' or letra=='G' or letra=='J' or letra=='Ã‘' or letra=='O' or letra=='P' or letra=='Q' or letra=='R' or letra=='U' or letra=='Y'):
            print("0")
        else:
            print("1")

curvi()
