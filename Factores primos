def factoresprimos():
    n=input("Deme un mumero: ")
    #Pruebo a dividirlo por los numeros que estan por debajo de el
    cont=n-1 #Donde debe de empezar
    while(cont>1):
    #Si el numero es divisor, se saca el cociente
        if(n%cont==0):
            print("Es divisible:)")
            cociente=n/cont
            print("" +str(cociente) +"")
            n=n/cont
            print("Nuevo numero: " +str(n))
        else:
            cont=cont-1 #cont--

factoresprimos()
